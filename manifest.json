{
  "name": "Google it!",
  "version": "1.0",
  "manifest_version":2,
  "permissions": [
    "activeTab"
  ],
  "description": "Click the button to Google the selected text",
  "icons":{
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
